{
	"name": "mel-miller/sb_twig_theme",
	"description": "Drupal theme to demo sb-twig-poc.",
	"type": "drupal-theme",
	"require": {
		"oomphinc/composer-installers-extender": "^2.0"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "mel-miller",
			"email": "melissa.miller@pantheon.io"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.drupal.org/8"
		}
	],
	"extra": {
		"installer-types": [
			"design-system"
		],
		"installer-paths": {
			"design-system/{$name}": [
				"type:design-system"
			]
		}
	}
}
